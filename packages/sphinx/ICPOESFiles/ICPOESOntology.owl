@prefix CheBI: <http://purl.obolibrary.org/obo/> .
@prefix ICPOESOntology: <https://cwrusdle.bitbucket.io/ICPOESOntology/> .
@prefix PMDCo: <https://w3id.org/pmd/co/> .
@prefix QUDT: <http://qudt.org/2.1/vocab/unit#> .
@prefix RDF: <https://www.w3.org/TR/2014/REC-rdf-schema-20140225/> .
@prefix ns1: <http://www.w3.org/2004/02/skos/core#> .
@prefix ns2: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

CheBI:CHEBI_24431 a rdfs:Class ;
    rdfs:label "CHEBI_24431" .

ICPOESOntology:ArgonFlowRate a rdfs:Class ;
    rdfs:label "ArgonFlowRate" ;
    rdfs:subClassOf PMDCo:TestingRate ;
    ns1:definition "The flow rate of argon in the ICPOES machine" ;
    PMDCo:unit QUDT:L-PER-MIN .

ICPOESOntology:Filter a rdfs:Class ;
    rdfs:label "Filter" ;
    rdfs:subClassOf PMDCo:ProcessingNode ;
    ns1:definition "a porous device for removing impurities or solid particles from a liquid or gas passed through it." ;
    PMDCo:unit QUDT:MicroM .

ICPOESOntology:FilteringProcess a rdfs:Class ;
    rdfs:label "FilteringProcess" ;
    rdfs:subClassOf PMDCo:Process ;
    ns1:definition "Process of filtering" .

ICPOESOntology:ICPOESProcess a rdfs:Class ;
    rdfs:label "ICPOESProcess" ;
    rdfs:subClassOf ICPOESOntology:SpectometryMeasuringProcess ;
    ns1:definition "A process related to ICPOES" .

ICPOESOntology:ICPOESSamplePrepProcess a rdfs:Class ;
    rdfs:label "ICPOESSamplePrepProcess" ;
    rdfs:subClassOf PMDCo:Process ;
    ns1:definition "Process of preparing a sample for an ICPOES Experiment" .

ICPOESOntology:ICPOESTestingMachine a rdfs:Class ;
    rdfs:label "ICPOESTestingMachine" ;
    rdfs:subClassOf PMDCo:TestingMachine ;
    ns1:definition "A testing machine used in an ICPOES experiment" .

ICPOESOntology:ICPOESView a rdfs:Class ;
    rdfs:label "ICPOESView" ;
    rdfs:subClassOf PMDCo:ValueObject ;
    ns1:definition "The configuration of the ICPOES machine (radial or axial)" .

ICPOESOntology:SampleFlowRate a rdfs:Class ;
    rdfs:label "SampleFlowRate" ;
    rdfs:subClassOf PMDCo:TestingRate ;
    ns1:definition "The flow rate of the sample in the ICPOES machine" ;
    PMDCo:unit QUDT:MilliL-PER-MIN .

ICPOESOntology:SpectometryMeasuringProcess a rdfs:Class ;
    rdfs:label "SpectometryMeasuringProcess" ;
    rdfs:subClassOf PMDCo:MeasuringProcess ;
    ns1:definition "The process by which you can measure the concentration of elements in a certain sample" .

ICPOESOntology:Spectrometer a rdfs:Class ;
    rdfs:label "Spectrometer" ;
    rdfs:subClassOf ICPOESOntology:ICPOESTestingMachine ;
    ns1:definition "A scientific instrument used to separate and measure spectral components of a physical phenomenon" .

ICPOESOntology:TestPieceName a rdfs:Class ;
    rdfs:label "TestPieceName" ;
    rdfs:subClassOf PMDCo:Identifier ;
    ns1:definition "Name of test piece for ICPOES experiment" .

ICPOESOntology:Volume a rdfs:Class ;
    rdfs:label "Volume" ;
    rdfs:subClassOf PMDCo:ValueObject ;
    ns1:definition "the amount of space that a substance or object occupies" ;
    PMDCo:unit QUDT:MilliL .

ICPOESOntology:dilutionFactor a rdfs:Class ;
    rdfs:label "dilutionFactor" ;
    rdfs:subClassOf PMDCo:SetPoint ;
    ns1:definition "Factor of dilution" .

ICPOESOntology:dilutionProcess a rdfs:Class ;
    rdfs:label "dilutionProcess" ;
    rdfs:subClassOf PMDCo:Process ;
    ns1:definition "Process of diluting" .

ICPOESOntology:isCalibrationSample a rdfs:Class ;
    rdfs:label "isCalibrationSample" ;
    rdfs:subClassOf PMDCo:ValueObject ;
    ns1:definition "Boolean value corresponding to whether the sample is for calibration of ICPOES machine" .

PMDCo:Dataset a rdfs:Class ;
    rdfs:label "Dataset" .

PMDCo:Date a rdfs:Class ;
    rdfs:label "Date" .

PMDCo:Duration a rdfs:Class ;
    rdfs:label "Duration" ;
    PMDCo:unit QUDT:s .

PMDCo:Identifier a rdfs:Class ;
    rdfs:label "Identifier" .

PMDCo:Laboratory a rdfs:Class ;
    rdfs:label "Laboratory" .

PMDCo:MeasuringProcess a rdfs:Class ;
    rdfs:label "MeasuringProcess" .

PMDCo:NodeSerialNumber a rdfs:Class ;
    rdfs:label "NodeSerialNumber" .

PMDCo:NodeSeries a rdfs:Class ;
    rdfs:label "NodeSeries" .

PMDCo:NonTransformativeAnalysisProcess a rdfs:Class ;
    rdfs:label "NonTransformativeAnalysisProcess" .

PMDCo:Process a rdfs:Class ;
    rdfs:label "Process" .

PMDCo:ProcessingNode a rdfs:Class ;
    rdfs:label "ProcessingNode" .

PMDCo:SetPoint a rdfs:Class ;
    rdfs:label "SetPoint" .

PMDCo:Specimen a rdfs:Class ;
    rdfs:label "Specimen" .

PMDCo:TestPiece a rdfs:Class ;
    rdfs:label "TestPiece" .

PMDCo:TestPieceInfo a rdfs:Class ;
    rdfs:label "TestPieceInfo" .

PMDCo:TestingMachine a rdfs:Class ;
    rdfs:label "TestingMachine" .

PMDCo:TestingRate a rdfs:Class ;
    rdfs:label "TestingRate" .

PMDCo:ValueObject a rdfs:Class ;
    rdfs:label "ValueObject" .

RDF:Bag a rdfs:Class ;
    rdfs:label "Bag" .

ICPOESOntology:Ontology a owl:Ontology ;
    rdfs:label "ICPOESOntology" ;
    ns2:creator "Alexander C. Harding Bradley",
        "Balashanmuga Priyan Rajamohan",
        "Daniel Struk",
        "Erika I. Barcelos",
        "Hayden Caldwell",
        "Jiana Kanbo",
        "Kiefer Lin",
        "Peter J. Hesketh",
        "Roger H. French" ;
    ns2:description "An ontology describing the Inductively coupled plasma-optical emission spectrometry domain." ;
    ns2:hasVersion "0.2" ;
    ns2:title "ICPOESOntology" .

PMDCo:characteristic a owl:ObjectProperty ;
    rdfs:domain ICPOESOntology:FilteringProcess,
        ICPOESOntology:ICPOESProcess,
        ICPOESOntology:Spectrometer,
        ICPOESOntology:dilutionProcess,
        PMDCo:Specimen,
        PMDCo:TestPiece ;
    rdfs:range ICPOESOntology:ArgonFlowRate,
        ICPOESOntology:Filter,
        ICPOESOntology:ICPOESView,
        ICPOESOntology:SampleFlowRate,
        ICPOESOntology:TestPieceName,
        ICPOESOntology:Volume,
        ICPOESOntology:dilutionFactor,
        ICPOESOntology:isCalibrationSample,
        PMDCo:Date,
        PMDCo:Duration,
        PMDCo:Laboratory,
        PMDCo:NodeSerialNumber,
        PMDCo:NodeSeries,
        PMDCo:TestPieceInfo,
        RDF:Bag .

PMDCo:executedBy a owl:ObjectProperty ;
    rdfs:domain ICPOESOntology:ICPOESProcess ;
    rdfs:range ICPOESOntology:Spectrometer .

PMDCo:input a owl:ObjectProperty ;
    rdfs:domain ICPOESOntology:ICPOESProcess,
        ICPOESOntology:ICPOESSamplePrepProcess,
        PMDCo:NonTransformativeAnalysisProcess ;
    rdfs:range PMDCo:Dataset,
        PMDCo:Specimen,
        PMDCo:TestPiece .

PMDCo:nextProcess a owl:ObjectProperty ;
    rdfs:domain ICPOESOntology:ICPOESSamplePrepProcess ;
    rdfs:range ICPOESOntology:ICPOESProcess .

PMDCo:output a owl:ObjectProperty ;
    rdfs:domain ICPOESOntology:ICPOESSamplePrepProcess,
        ICPOESOntology:Spectrometer,
        PMDCo:NonTransformativeAnalysisProcess ;
    rdfs:range PMDCo:Dataset,
        PMDCo:TestPiece .

PMDCo:resource a owl:DatatypeProperty ;
    rdfs:domain ICPOESOntology:ArgonFlowRate ;
    rdfs:range xsd:string .

PMDCo:subordinateProcess a owl:ObjectProperty ;
    rdfs:domain ICPOESOntology:ICPOESSamplePrepProcess ;
    rdfs:range ICPOESOntology:FilteringProcess,
        ICPOESOntology:dilutionProcess .

PMDCo:value a owl:DatatypeProperty ;
    rdfs:domain ICPOESOntology:ArgonFlowRate,
        ICPOESOntology:Filter,
        ICPOESOntology:ICPOESView,
        ICPOESOntology:SampleFlowRate,
        ICPOESOntology:TestPieceName,
        ICPOESOntology:Volume,
        ICPOESOntology:dilutionFactor,
        PMDCo:Date,
        PMDCo:NodeSerialNumber,
        PMDCo:NodeSeries,
        PMDCo:TestPieceInfo ;
    rdfs:range xsd:dateTime,
        xsd:float,
        xsd:integer,
        xsd:string .

RDF:_1 a owl:ObjectProperty ;
    rdfs:domain RDF:Bag ;
    rdfs:range CheBI:CHEBI_24431 .

